<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>
		
		<!-- use thumbnails for base64 images -->
		<!-- this may slowdown viewing tests -->
		<thumbnailForBase64>true</thumbnailForBase64>
		
		<!-- title of the document -->
		<documentTitle>Greatify Automation Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>End-to-End business stories of Greatify ERP Automation
		</reportName>

		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>top</testViewChartLocation>


		<!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
		<dateFormat>dd-mm-yyyy</dateFormat>

		<!-- global time format override -->   <!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
                
            ]]>
		</styles>
	</configuration>
</extentreports>